{
  "manifest_version": 2,
  "name": "UCSC Xena Chrome Extension",
  "description": "Extend dcc.icgc.org for visualization of expression, variant, and phenotype data",
  "version": "1.1",
  "background": {
	"scripts": ["control.js"]
  },
  "browser_action": {
    "default_icon": "icon-16.png",
	"default_title": "UCSC Xena",
	"default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
	"webRequest",
	"webRequestBlocking",
	"*://dcc.icgc.org/",
    "activeTab"
  ],
  "web_accessible_resources": [
	  "scripts.js",
	  "advanced.html",
	  "advanced.expression.html",
	  "*.woff",
	  "*.woff2",
	  "*.ttf",
	  "*.svg"
  ],
  "icons": {"16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"},
  "key": ""
}
